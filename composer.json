{
    "name": "opencast-ilias/opencast-event",
    "description": "ILIAS Opencast Event Object Plugin",
    "type": "project",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Farbod Zamani Boroujeni",
            "email": "zamani@elan-ev.de"
        }
    ],
    "support": {
        "email": "zamani@elan-ev.de"
    },
    "minimum-stability": "beta",
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "phpcompatibility/php-compatibility": "*",
        "phpunit/phpunit": "^9.6",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "classmap": [
            "./classes/",
            "./src/"
        ],
        "files": [
            "../OpenCast/vendor/autoload.php"
        ]
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload-dev": {
        "psr-4": {
            "elanev\\OpencastEvent\\Tests\\": "tests/"
        }
    }
}
